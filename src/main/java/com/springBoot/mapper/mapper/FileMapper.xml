<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.springBoot.mapper.mapper.FileMapper">
    <select id="findAllFile" parameterType="map" resultType="com.springBoot.entity.File">
        select * from t_file
    </select>

    <delete id="deleteFile" parameterType="map">
        DELETE from t_file where id = #{id}
    </delete>

    <insert id="saveFile" parameterType="map">
        INSERT INTO t_file(file_name,file_path,date,time) VALUE(#{file.fileName},#{file.filePath},#{file.date},#{file.time})
    </insert>
</mapper>